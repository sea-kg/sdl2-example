{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/src/strategies/shooting",
            "${workspaceFolder}/src/utils",
            "${workspaceFolder}/src/game",
            "${workspaceFolder}/src/ai",
            "${workspaceFolder}/src/yassets",
            "${workspaceFolder}/src/render",
            "${workspaceFolder}/src/yservices",
            "${workspaceFolder}/src/Roads2DGenerator",
            "${workspaceFolder}/src/SimpleNeuralNetworkCpp",
            "${workspaceFolder}/src/render",
            "${workspaceFolder}/src/controllers",
            "${workspaceFolder}/src/ycore",
            "${workspaceFolder}/src"
        ],
        "myDefines": [
            "DEBUG",
            "MY_FEATURE=1"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/gcc",
            "compilerArgs": [],
            "intelliSenseMode": "gcc-x64",
            "includePath": [
                "${myIncludePath}",
                "/usr/include"
            ],
            "defines": ["${myDefines}"],
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "configurationProvider": "ms-vscode.cmake-tools",
            "forcedInclude": [],
            "browse": {
              "path": ["${myIncludePath}", "/usr/include", "${workspaceFolder}"],
              "limitSymbolsToIncludedHeaders": true,
              "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
          },
          {
            "name": "Win32",
            "compilerPath": "C:/Program Files (x86)/Microsoft Visual Studio/2019/Community/VC/Tools/MSVC/14.28.29333/bin/Hostx64/x64/cl.exe",
            "intelliSenseMode": "msvc-x64",
            "includePath": ["${myIncludePath}"],
            "defines": ["${myDefines}", "_WINDOWS"],
            "cStandard": "c18",
            "cppStandard": "c++17",
            "windowsSdkVersion": "10.0.19041.0",
            "browse": {
              "path": ["${myIncludePath}", "${workspaceFolder}"]
            }
          }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}